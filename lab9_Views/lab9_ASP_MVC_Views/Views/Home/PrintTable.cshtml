@model List<List<string>>

<table class="table table-hover table-bordered">
    <thead>
        <tr>
            @foreach (var item in @Model.First())
            {
                <th>@item</th>
            }
        </tr>
    </thead>
    <tbody>
        @for (int i = 1; i < @Model.Count; i++)
        {
            <tr>
                @foreach (var item in @Model[i])
                {
                    <td>@item</td>
                }
            </tr>
        }
    </tbody>
</table>
